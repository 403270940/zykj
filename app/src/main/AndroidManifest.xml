<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.zykj.activities" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    package="com.liyongyue.zykj" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data android:name="xposedmodule"      android:value="true" />
        <meta-data android:name="xposeddescription" android:value="这个可以伪装手机软件显示的手机型号" />
        <meta-data android:name="xposedminversion"  android:value="42+" />
        <activity
            android:name="com.zykj.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.zykj.activities.Param"
            android:label="@string/title_activity_param" >
        </activity>

    </application>

</manifest>
